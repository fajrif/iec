<%= simple_form_for @setting, url: admins_setting_path, method: (@setting.new_record? ? :post : :put) do |f| %>
	<%= f.error_notification %>
	<fieldset>
		<%= f.input :admin_investor, as: :string %>
		<%= f.input :admin_media, as: :string %>
		<%= f.input :admin_partner, as: :string %>
	</fieldset>
	<%= f.error :base %>
	<div class="form-actions">
		<%= f.button :submit, :class => 'btn btn-secondary' %>
		<%= link_to "Back", admins_setting_path, :class => 'btn btn-light' %>
	</div>
<% end %>
